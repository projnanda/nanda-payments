{
  "name": "unified-ai-agent-marketplace",
  "version": "1.0.0",
  "description": "Unified AI Agent Marketplace with NANDA Points and Ethereum payments on Radius blockchain",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node --enable-source-maps dist/server.js",
    "build": "tsc -p tsconfig.json",
    "seed": "tsx src/seed.ts",
    "demo": "tsx src/demo.ts",
    "test": "jest",
    "test:points": "jest --testPathPattern=points",
    "test:blockchain": "jest --testPathPattern=blockchain",
    "test:reputation": "jest --testPathPattern=reputation",
    "test:integration": "jest --testPathPattern=integration",
    "test:demo": "tsx src/demo.ts",
    "test:load": "artillery run test/load/load-test.yml",
    "test:stress": "artillery run test/load/stress-test.yml",
    "deploy:radius": "hardhat run scripts/deploy.js --network radius_testnet",
    "deploy:local": "hardhat run scripts/deploy.js --network hardhat",
    "deploy:all": "npm run deploy:radius && npm run build && npm run start",
    "dev:frontend": "cd frontend && npm start",
    "dev:services": "./start_services.sh",
    "compile": "hardhat compile",
    "clean": "hardhat clean"
  },
  "keywords": [
    "ai-agents",
    "marketplace",
    "blockchain",
    "points",
    "ethereum",
    "radius",
    "reputation",
    "payments"
  ],
  "author": "NANDA Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.8.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.5.1",
    "morgan": "^1.10.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "web3": "^4.3.0",
    "winston": "^3.11.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.12",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "artillery": "^2.0.0",
    "hardhat": "^2.19.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
